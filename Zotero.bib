	pmcid = {PMC64998},
	keywords = {Biomarkers, Tumor, Cluster Analysis, Gene Expression Profiling, Humans, Multigene Family, Neoplasms},
	pages = {15149--15154}
}

@article{anders_differential_2010,
	title = {Differential expression analysis for sequence count data},
	volume = {11},
