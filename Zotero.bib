@article{liu_cudasw++_2013,
	title = {{CUDASW}++ 3.0: accelerating {Smith}-{Waterman} protein database search by coupling {CPU} and {GPU} {SIMD} instructions},
	volume = {14},
	issn = {1471-2105},
	shorttitle = {{CUDASW}++ 3.0},
	url = {http://dx.doi.org/10.1186/1471-2105-14-117},
	doi = {10.1186/1471-2105-14-117},
